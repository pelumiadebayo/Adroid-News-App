<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.news">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Politics" />
        <activity android:name=".Entertainment" />
        <activity android:name=".Technology" />
        <activity android:name=".Business" />
        <activity android:name=".News" />
        <activity
            android:name=".NewsListAdapter"
            android:label="@string/title_activity_news_adapter"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>